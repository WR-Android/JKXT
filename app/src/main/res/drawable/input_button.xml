<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorTransparent" />
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <stroke android:width="3px" android:color="#999999" />
            <padding android:top="1px" />
            <size android:width="200dp" android:height="60dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorTransparent" />
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <stroke android:width="3px" android:color="@color/colorTransparent" />
            <padding android:top="1px" />
            <size android:width="200dp" android:height="60dp" />
        </shape>
    </item>
</selector>
